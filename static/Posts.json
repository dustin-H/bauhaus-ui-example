						"path": "hallo.welt.select",
						"options": {
							"a": "A Auswahl",
							"b": "B Auswahl",
							"c": "C Auswahl"
						}
					}
				}, {
					"name": "Validator",
					"props": {
						"path": "hallo.welt.select",
						"required": true,
						"regex": "[a-b]",
						"failMessage": "This needs to be A or B!"
